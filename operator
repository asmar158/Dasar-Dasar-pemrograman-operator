include<iostream>
using namespace std;
int main ()
{
  int a;
  int b;
  int c;
  
  cout<<"masukkan nilai a=";
  cin>>a;
  cout<<"masukkan nilai b=";
  cin>>b;
  cout<<"masukkan nilai a=";
  cin>>c;
  
  cout<<"\nhasil yang dimasukkan adalah :"<<endl<<endl;
  cout<<"A or B ="<<(a||b)<< endl;
  cout <<"A or C="<<(a||c)<<endl;
  cout<<"F1=(A or B) AND (A or C)="<<((a||b)&&(a||c))<<endl;
  
  return 0;
  
  }
